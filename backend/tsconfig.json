{
    "compilerOptions": {
        "target": "ES2019",
        "module": "ESNext",
        "lib": [
            "ES2019"
        ],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "types": [
            "google-apps-script"
        ]
    },
    "include": [
        "src/**/*"
    ]
}